        <script type="text/javascript">
                      var cv = document.getElementById('myCanvas');
                      var context = cv.getContext('2d');

                      context.clearRect(0, 0, cv.width, cv.height);

                      $('.mapPart').mouseover(function () {
                          var coordStr = this.getAttribute('coords');
                          // draw
                          drawPolygon(context, coordStr);
                      });

                      $('.mapPart').mouseout(function () {
                          // clear
                          context.clearRect(0, 0, cv.width, cv.height);
                      });

                      $('.mapPart').mousedown(function () {
                          var altStr = this.getAttribute('alt');
                          var titleStr = this.getAttribute('title');
                          selContent(altStr, titleStr);
                      });

                      $('.mapPart').mouseup(function () {
                          var altStr = this.getAttribute('alt');
                          var titleStr = this.getAttribute('title');
                          selContent(altStr, titleStr);
                      });

                      //need to parse this div, get all the alt names, so I can make them all inactive during mouseclick, so I can then only make the one that is clicked active. Probably better to use JQuery for this
                      var groupoArr = []

                      $( document ).ready(function() {
                        console.log( 'ready!' );

                        
                        $('.mapPart').each(function(index,value) {
                            //console.log(index + ":" + $(this).attr('alt'));
                            groupoArr.push($(this).attr('alt'));
                        });
                        
                        //console.log(groupoArr);

                      }); //end of JQuery document ready function
                      
                      //console.log(groupoArr);

                      //draw poly and
                      function drawPolygon(hdc, coOrdStr) {
                          var mCoords = coOrdStr.split(',');
                          var i, n;
                          n = mCoords.length;

                          hdc.beginPath();
                          hdc.moveTo(mCoords[0], mCoords[1]);
                          for (i = 2; i < n; i += 2) {
                              hdc.lineTo(mCoords[i], mCoords[i + 1]);
                          }
                          hdc.lineTo(mCoords[0], mCoords[1]);
                          hdc.lineWidth="12";
                          hdc.strokeStyle="#ff0000"; // red path
                          hdc.globalAlpha=".4"  // transparency
                          hdc.stroke();

                      }
                      //select content to display
                      function selContent(groupId,projectTitle) {
                        document.getElementById('group-project').innerHTML = "<li><a data-toggle='tab' href=" + groupId + ">" + projectTitle + "</a></li>";

                        //console.log("groupId: ")
                        //console.log(groupId)

                        for (i = 0; i < groupoArr.length; i++) { 
                          document.getElementById(groupoArr[i]).className = "tab-pane fade";
                        }
                        
                        document.getElementById(groupId).className = "tab-pane fade in active";
                      }
                  </script>

